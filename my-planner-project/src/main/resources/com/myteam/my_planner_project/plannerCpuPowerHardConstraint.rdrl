package com.myteam.my_planner_project;

import java.lang.Number;

rule "plannerCpuPowerHardConstraint"
	dialect "mvel"
	when
		$c : Computer( $cpu : cpu)
		Number( intValue() > $cpu ) from accumulate ( Process( computer == $c , $reqCpu : requiredCpu),
			sum($reqCpu)) 
	then
		scoreHolder.addHardConstraintMatch(kcontext, $reqCpu - $cpu);
end
